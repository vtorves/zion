
#DEMANDAS GERAIS / ABERTAS PRIVADAS
<div class="table-responsive">
      <table class="table table-bordered">
        <thead>
          <tr>
            <th>STATUS</th>
            <th>TICKET</th>
            <th>GRUPO</th>
            <th>CRIADOR</th>
            <th>ASSUNTO</th>
            <th>LOCAL</th>
            <th>DETALHES</th>
          </tr>
        </thead>
        <tbody>
        <%= @account.inspect %>
        <pre>
        <%= @tickets.inspect %>
        </pre>  
        <% @tickets.each do |ticket| %>
          <tr>
            <td><%= ticket.status %></td>
            <td><%= ticket.number_only %></td>
            <td><%= ticket.group.name %></td>
            <td><%= create_for(ticket.account_id) %>  </td>
            <td><%= ticket.subject %> / <%= ticket.assigned %></td>
            <td><%= ticket.local %></td>
            <td>        <%= link_to ticket.local, ticket_reply_path(ticket.id) %>
</td>          </tr>
    <% end %>

         </tbody>
      </table>
    </div>

